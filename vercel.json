{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/myguestservices"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/assets/(.*)",
      "destination": "/assets/$1"
    },
    {
      "source": "/(.*\\.js)",
      "destination": "/$1"
    },
    {
      "source": "/(.*\\.css)",
      "destination": "/$1"
    },
    {
      "source": "/(.*\\.ico)",
      "destination": "/$1"
    },
    {
      "source": "/(.*\\.png)",
      "destination": "/$1"
    },
    {
      "source": "/(.*\\.jpg)",
      "destination": "/$1"
    },
    {
      "source": "/(.*\\.svg)",
      "destination": "/$1"
    },
    {
      "source": "/(.*\\.woff)",
      "destination": "/$1"
    },
    {
      "source": "/(.*\\.woff2)",
      "destination": "/$1"
    },
    {
      "source": "/(.*\\.ttf)",
      "destination": "/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}

